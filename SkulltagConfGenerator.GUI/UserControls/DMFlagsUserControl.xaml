<UserControl x:Class="SkulltagConfGenerator.GUI.UserControls.DMFlagsUserControl"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:flags="clr-namespace:SkulltagConfGenerator.GUI.ViewModel.Flags"
			 xmlns:controls="clr-namespace:SkulltagConfGenerator.GUI.UserControls"
			 mc:Ignorable="d">	
	<UserControl.DataContext>
		<flags:DMFlagViewModel />		
	</UserControl.DataContext>
	<StackPanel>
		<controls:FlagTemplate />
		<DockPanel LastChildFill="False">
			<Button Content="OK" DockPanel.Dock="Right" Margin="0, 0, 8, 0" Width="40" />
		</DockPanel>
	</StackPanel>
</UserControl>
